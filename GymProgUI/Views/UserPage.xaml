<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GymProgUI.Views.UserPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="User Name" Text="{Binding UserName}"> </Entry>
            <Entry Placeholder="Password" IsPassword="True" Text="{Binding Password}"></Entry>
            <Button Text="Delete" Command="{Binding Delete}"></Button>
            <Button Text="Update" Command="{Binding Update}"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>